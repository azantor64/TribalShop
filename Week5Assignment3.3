try:
    sc = float(input("Enter your score between 0.0 and 1.0 "))
   
except:
    if sc > 1.0 or sc < 0.0:
        print ("Error, the score should be in the range between 0.0 and 1.0")
    quit()
    
if sc >= 0.9: 
    print ('A')
elif sc >= 0.8:
    print ('B')
elif sc >= 0.7:
    print ('C')
elif sc < 0.6:
    print ('F')
